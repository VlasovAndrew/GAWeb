@model GeneticAlgorithm.Entities.FindingVertexResponse
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Результаты вычислений";
}

<div class="row justify-content-center mt-4">
    <div class="col-5 bg-white p-3">
        <h2 class="text-center"> Результаты анализа </h2>
        <h4> Время анализа: @Model.Time </h4>
        <h4> Радиус графа: @Model.R </h4>
        <h4> Центральные вершины графа: 
            @foreach (var v in Model.Center)
            {
                <span> @v </span>
            }
        </h4>
        <div class="text-center"> *Алгортим не гарантирует точное решение. </div>
    </div>
</div>
