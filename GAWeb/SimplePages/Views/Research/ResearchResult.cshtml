@model GeneticAlgorithm.Entities.Response.AlgorithmResultResponse
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "ResearchResult";
}

    <div class="row justify-content-center mt-2">
        <div class="col-6 bg-white p-3">
            <h4 class="text-center"> Работа алгоритма с выбранными параметрами </h4>
            <div> Pc = @Model.ResearchRequest.Pc </div>
            <div> Pm = @Model.ResearchRequest.Pm </div>
            <div> Размер популяции = @Model.ResearchRequest.PopulationSize </div>
            <div>
                Среднее время работы: @Model.AlgorithmResponse.AvgTime сек
            </div>
            <div>
                Процент ошибок: @Model.AlgorithmResponse.Error %
            </div>
        </div>
    </div>
